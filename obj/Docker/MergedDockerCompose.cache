name: dockercompose6468427737071505931
services:
  creditproposal.worker:
    build:
      context: E:\ProcessoSeletivo\workers-rabbitMQ\src\Services\CreditProposal.Worker
      dockerfile: Dockerfile
    depends_on:
      mongodb:
        condition: service_started
        required: true
      rabbitmq:
        condition: service_started
        required: true
    environment:
      DOTNET_ENVIRONMENT: Development
    networks:
      proposal-network: null
  customer.worker:
    build:
      context: E:\ProcessoSeletivo\workers-rabbitMQ\src\Services\Customer.Worker
      dockerfile: Dockerfile
    depends_on:
      mongodb:
        condition: service_started
        required: true
      rabbitmq:
        condition: service_started
        required: true
    environment:
      DOTNET_ENVIRONMENT: Development
    networks:
      proposal-network: null
  mongodb:
    container_name: mongodb
    image: mongo
    networks:
      proposal-network: null
    ports:
    - mode: ingress
      target: 27017
      published: "27017"
      protocol: tcp
    restart: always
  proposalcreditcard.api:
    build:
      context: E:\ProcessoSeletivo\workers-rabbitMQ\src\Modules\ProposalCreditCard.API
      dockerfile: Dockerfile
    depends_on:
      mongodb:
        condition: service_started
        required: true
      rabbitmq:
        condition: service_started
        required: true
    networks:
      proposal-network: null
    ports:
    - mode: ingress
      target: 80
      published: "5000"
      protocol: tcp
  rabbitmq:
    hostname: rabbitmq
    image: rabbitmq:3-management
    networks:
      proposal-network: null
    ports:
    - mode: ingress
